<h2>EDL to XLSX BETA</h2>
@using (Html.BeginForm("UploadFileXml",
                        "UploadFiles",
                        FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
{
    <label for="file">Upload EDL File:</label>
    <input type="file" name="file" id="file" /><br><br>
    <input type="submit" value="Upload File" />
    <br><br>
    @ViewBag.Message
}


